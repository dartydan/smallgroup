{
  "expo": {
    "name": "Small Group",
    "slug": "smallgroup",
    "scheme": "smallgroup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sglogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/sglogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.zoomi.smallgroup",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "supportsTablet": true
    },
    "android": {
      "package": "com.zoomi.smallgroup",
      "adaptiveIcon": {
        "foregroundImage": "./assets/sglogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/sglogo.png"
    },
    "extra": {
      "eas": {
        "projectId": "c940e2b3-ee93-48e6-b1cc-ab486ad2d8b2"
      }
    },
    "owner": "zoomi"
  }
}
